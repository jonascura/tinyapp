<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <!-- new code -->
    <% if (user_id) { %>
      <form class="form-inline" action="/logout" method="POST" >
      <div class="form-group mb-2">
        <label for="logout">Logged in as: <%= user_id.email %>!</label>
        <button type="submit" class="btn btn-outline-dark">Logout</button>
      </div>
      </form> 
    <% } else { %>  
      <form class="form-inline" action="/linkToReg" method="POST">
        <div class="form-group mb-2">
          <button type="submit" class="btn btn-primary">Register</button>
        </div>
      </form>
      <form class="form-inline" action="/linkToLogin" method="POST">
      <div class="form-group mb-2">
        <button type="submit" class="btn btn-warning">Log In</button>
      </div>
    </form>
    <!-- end new code -->
  <% } %>
  </nav>
</header>